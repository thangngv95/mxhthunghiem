<% provide :title, t("list-users") %>
<div class="row">
  <div class="col-md-10 col-md-offset-1 middle-follow-users">
    <%= will_paginate %>
    <h1><%= t "list-users" %></h1>
    <% if @users %>
      <% @users.each do |user| %>
        <%= render "user", user: user %>
      <% end %>
    <% else %>
      <h3 ><%= t :no_content %></h3>
    <% end %>
    <%= will_paginate %>
  </div>
</div>
